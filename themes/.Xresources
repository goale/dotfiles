Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! special
*.foreground:   #ffffff
*.background:   #666666
*.cursorColor:  #ffffff

! black
*.color0:       #2e3440
